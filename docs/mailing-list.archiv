/*
 * JANUS oder ZNETZ Login fuer UNIX
 *
 *  ACHTUNG: eine sichere Funktion bei der Annahme von Z-NETZ oder JANUS
 *           Anrufen durch eine UNIX System kann derzeit nicht gewaehrleistet
 *           werden!
 */

#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>
#include <setjmp.h>
#include <signal.h>
#include <dirent.h>
#include "header.h"
#include "hd_def.h"
#include "hd_nam.h"
#include "config.h"
#include "version.h"
#include "track.h"
#include "line.h"
#include "boxstat.h"
#include "ministat.h"
#include "xprog.h"

static jmp_buf timeout, nocarrier;
static char *tty = "/dev/tty";

FILE *logfile;
static int ist_net38 = 0;

#define NAK	0x15
#define ACK	0x06
#define echo    1
#define no_echo 0  

void handle_timeout(int code)
{
	longjmp(timeout, 1);
}

void handle_nocarrier(int code)
{
	longjmp(nocarrier, 1);
}

void getiln(char *p, int iecho)
{
	int z;

	while (1) {
		z = getchar();
		if (z == '\r') break;
		if (isprint(z))
			*p++ = z;
                if (iecho)
                        putc(z, stdout);
       	}
	*p = '\0';
	fputs("\r\n", stdout);
}

void reject(char *prog, char *sysname, char *passwd)
{
	fputs("Netzzugriff verweigert.\r\n", stdout);
	fprintf(logfile, "%s: Fehlversuch (System %s Passwort %s)\n", prog, sysname, passwd);
	fclose(logfile);
	lock_device(0, tty);
	exit(0);
}

/*
 *   Hauptprogramm, Gesamtstruktur
 */ 
main(int argc, char **argv)
{
	char sysname[80];
	char passwd[80];
	char filename[FILENAME_MAX];
	char tmpname[FILENAME_MAX];
	char outname[FILENAME_MAX];
	char *arcer, *arcerin, *transfer, sernr[6];
	int i, j, z;
	FILE *f;
	header_p hd, p;
	
	/*
	 *  Da dieses Programm Login-Shell ist, muss die Fehlerausgabe
	 *  irgendwo hin dirigiert werden...
	 */
	logfile = fopen("/tmp/zlog", "a");
	if (!logfile) {
		logfile = fopen("/dev/null", "w");
		if (!logfile) {
			printf("Sorry - kann Logfile nicht schreiben...\n");
			return 10;
		}
	}

	/*
	 *  Wie wurde das Programm aufgerufen?
	 */
	ist_net38 = strstr(argv[0], "janus") == 0;
	
	/*
	 *  Konfiguration einlesen.
	 */
	minireadstat();
	
	/*
	 *  Verschiedene Abbruch-Gruende vorbereiten
	 */
	if (setjmp(timeout)) {
		fputs("\nABBRUCH: Timeout\n", logfile);
		fclose(logfile);
		return 1;
	}
	if (setjmp(nocarrier)) {
		fputs("\nABBRUCH: Gegenstelle hat aufgelegt!\n", logfile);
		fclose(logfile);
		return 2;
	}
	signal(SIGHUP, handle_nocarrier);
	signal(SIGALRM, handle_timeout);

	/*
	 *  Hier beginnt der eigentliche Datenaustausch
	 */
	set_rawmode(fileno(stdin));
	set_rawmode(fileno(stdout));

	tty = ttyname(0);
	lock_device(1, tty);

	alarm(45);
ask_sysname:
	sleep(1); fflush(stdin);
	fputs("Systemname: ", stdout); fflush(stdout);
	getiln(sysname,echo);
	strlwr(sysname);
        switch (ist_net38) {
        case 1: if (strcmp(sysname, "zerberus") == 0) goto ask_sysname; break;
        case 0: if (strcmp(sysname, "janus") == 0) goto ask_sysname; break;
        }    
ask_passwort:
	sleep(1); fflush(stdin);
	fputs("Passwort: ", stdout); fflush(stdout);
	getiln(passwd,no_echo);
        switch (ist_net38) { 
	case 1:  if (stricmp(passwd, "zerberus") == 0) goto ask_sysname; break;
        case 0:  if (stricmp(passwd, "janus") == 0) goto ask_sysname; break;
        }
        if (stricmp(sysname, passwd) == 0) goto ask_passwort;
	alarm(0);

	sprintf(filename, "%s/%s", systemedir, sysname);
	f = fopen(filename, "rb");
	if (!f) {
		fprintf(logfile, "Unbekanntes System: %s\n", filename);
		reject(argv[0], sysname, passwd);
	}
	hd = rd_para(f);
	fclose(f);
	p = find(HD_SYS, hd);
	if (!p || stricmp(p->text, sysname) != 0) {
		fprintf(logfile, "Illegale Systemdatei: Kein SYS: Header oder falscher Name: %s\n", filename);
		reject(argv[0], sysname, passwd);
	}

	p = find(HD_PASSWD, hd);
	if (!p || stricmp(p->text, passwd) != 0) {
		fputs("Passwort falsch:\n", logfile);
		reject(argv[0], sysname, passwd);
	}

	p = find(HD_ARCEROUT, hd);
	if (!p) {
		fputs("Kein ARCer (ausgehend) definiert!\n", logfile);
		reject(argv[0], sysname, passwd);
	}
	arcer = p->text;
	strlwr(arcer);

	p = find(HD_ARCERIN, hd);
	if (!p) {
		fputs("Kein ARCer (eingehend) definiert!\n", logfile);
		reject(argv[0], sysname, passwd);
	}
	arcerin = p->text;
	strlwr(arcerin);

	p = find(HD_PROTO, hd);
	if (!p) {
		fputs("Kein Uebertragungsprotokoll definiert!\n", logfile);
		reject(argv[0], sysname, passwd);
	}
	transfer = p->text;
	strlwr(transfer);

	fputs("Running ARC....\r\n"
	     "Running ARC....\r\n"
	     "Running ARC....\r\n"
	     "Running ARC....\r\n", stdout);
	fprintf(logfile, "%s: System %s erfolgreich eingelogt [%s]\n", argv[0], sysname, tty);
/*
 * aus Zugriffsgruenden (owner) wird die sache nach netcalldir/system/prozessnummer verlegt
 */
	sprintf(tmpname, "%s/%s/%d", netcalldir, sysname, getpid());
	mkdir(tmpname, 0777);

	if (setjmp(timeout)) {
		fputs("\nABBRUCH: Timeout\n", logfile);
		fclose(logfile);
/*
 * Bei Fehlern wird das NC-Directory nun nicht mehr mit Dateien ( Directory )  vollgemuellt
 */
                chdir("/");
                rmdir(tmpname);
                return 1;
	}
	if (setjmp(nocarrier)) {
		fputs("\nABBRUCH: Gegenstelle hat aufgelegt!\n", logfile);
		fclose(logfile);
                chdir("/");
                rmdir(tmpname);
		return 2;
	}

	chdir(tmpname);
	sprintf(outname, "%s/called.%s", tmpname, arcer);
	sprintf(filename, "%s/%s/%s.%s", netcalldir, sysname, sysname, arcer);
	if (access(filename, R_OK) != 0) {
		f = fopen(outname, "wb");
		if (!f) {
			fprintf(logfile, "Kann Netcall %s nicht erzeugen\n", outname);
			fclose(logfile);
			return 1;
		}
                fprintf(logfile,"CALLED.%s neu angelegt\n", arcer);
		fputs("\r\n", f);
		fclose(f);
	} else if(rename(filename, outname)) {
		        fprintf(logfile, "Umbenennen: %s -> %s fehlgeschlagen\n", filename, outname);
	                fclose(logfile);
		        return 1;
        }
           
	sprintf(filename, "caller.%s", arcer);
	sprintf(outname, "called.%s", arcer);

	if (setjmp(timeout)) {
		fputs("\nABBRUCH: Timeout\n", logfile);
		goto cleanup;
	}
	if (setjmp(nocarrier)) {
		fputs("\nABBRUCH: Gegenstelle hat aufgelegt!\n", logfile);
		goto cleanup;
	}

	/*
	 *  Man glaubt es kaum, aber einige Programme schaffen es nicht,
	 *  ein Packzeit von 0 Sekunden der Gegenseite zu verstehen...
	 */
	sleep(3);

	alarm(30);	
	for (i=1; i<7; i++) {
		putchar(NAK); fflush(stdout);
		z = 0;
		for (j=0; j<5; j++) 
			sernr[j] = getchar();
		if ((char)(sernr[0]+sernr[1]+sernr[2]+sernr[3]) == sernr[4]) {
			alarm(0);
			putchar(ACK); fflush(stdout);
			sernr[4] = '\0';
			fprintf(logfile, "Seriennummer: %s\n", sernr);
			if (recvfile(transfer, filename)) {
				fprintf(logfile, "Empfang der Daten fehlgeschlagen!\n");
				break;
			}
			if (sendfile(transfer, outname)) {
				fprintf(logfile, "Versand der Daten fehlgeschlagen\n");
				break;
			}

			/* Fertig, Modem auflegen */
			signal(SIGHUP, SIG_IGN);
			fclose(stderr);
			fclose(stdin);
			hangup(fileno(stdout));
			fclose(stdout);
			fclose(logfile);
			lock_device(0, tty);

			/* Netcall war erfolgreich, also Daten loeschen */
			remove(outname);

			/*
			 * Und empfangene Daten (im Hintergrund) einlesen,
			 * das Modem wird sofort wieder freigegeben.
			 */
			if (fork() == 0) {
				/* Ich bin child */
				import_all(arcerin, sysname, ist_net38);
				chdir ("/");
				rmdir(tmpname);
			}

			return 0;
		}
	}

cleanup:
	alarm(0);
	fprintf(logfile, "Netcall fehlgeschlagen\n");
	lock_device(0, tty);

	/* Jetzt muessen die Daten zurueck aus dem temporaeren Verzeichnis
	 * ins Haupt-Netcall-Verzeichnis
	 */
	sprintf(filename, "%s/%s/%s.%s", netcalldir, sysname, sysname, arcer);
	sprintf(outname, "%s/called.%s", tmpname, arcer);
        fprintf(logfile,"O %s,F %s,T %s\n",outname,filename,tmpname);
        if (access(filename, R_OK) != 0) {
		/*
		 * Der einfache Fall: es ist noch nichts neues da,
		 * wir koennen die ARC Datei an die urspruengliche
		 * Stelle zurueckschieben.
		 */
		rename(outname, filename);
	} else {
		/*
		 * Uups, da hat schon jemand wieder gearct - dann packen
		 * wir die Daten einfach wieder im Sammel-Verzeichnis aus.
		 * Sie werden dann beim naechsten PreARC Lauf miterfasst.
		 */
		chdir(netcalldir);
		chdir(sysname);
		call_auspack(arcer, outname);
		remove(outname);
	}
	chdir("/");
	rmdir(tmpname);

	fclose(logfile);

	return 0;
}
****************************************************************************

Entschuldigt die lange Liste, villeicht hilft es einigen doch, so langsam
Konfigurationsfehler des uconn auszumerzen.

Gruß bernd

