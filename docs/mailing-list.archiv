const struct predefd *
predefd_header (register const char *str, register int len)
{
  if (len <= MAX_WORD_LENGTH && len >= MIN_WORD_LENGTH)
    {
      register int key = hash (str, len);

      if (key <= MAX_HASH_VALUE && key >= 0)
        {
          register int index = lookup[key];

          if (index >= 0 && index < MAX_HASH_VALUE)
            {
              register const char *s = wordlist[index].header;

              if (*s == *str && !strcmp (str + 1, s + 1))
                return &wordlist[index];
            }
          else if (index < 0 && index >= -MAX_HASH_VALUE)
            return 0;
          else
            {
              register int offset = key + index + (index > 0 ? -MAX_HASH_VALUE : MAX_HASH_VALUE);
              register const struct predefd *base = &wordlist[-lookup[offset]];
              register const struct predefd *ptr = base + -lookup[offset + 1];

              while (--ptr >= base)
                if (*str == *ptr->header && !strcmp (str + 1, ptr->header + 1))
                  return ptr;
            }
        }
    }
  return 0;
}


/*
 *  hash.epilogue:
 *
 *    Wird hinter die Ausgabe von "gperf" gehaengt und vervollstaendigt
 *    die Lookup-Funktionen.
 *
 * ------------------------------------------------------------------------
 * Letzte Aenderung: martin@sisyphos.owl.de, Thu Apr 15 13:33:46 1993
 */

#include <stdio.h>
#include <stdlib.h>
#include "header.h"
#include "uulog.h"
#include "hd_nam.h"

const char *headers[] = {
#include "hd_nlist.h"
NULL
};

extern char * strupr(char *);

unsigned identify(const char *name)
{
	static char hname[100];
	struct predefd *p;

	strcpy(hname, name);
	strupr(hname);

	p = (struct predefd *)predefd_header(hname, strlen(hname));

	return p ? p->code : TOTAL_KEYWORDS + 23;
}


#ifdef __GNUC__
inline
#endif
header_p internal_add_header(char *inhalt, int code, header_p ptr, const char *header_name)
{
	header_p p, last, new;

	/* Einfgestelle suchen */
	last = NULL;
	for (p = ptr; p && (p->code < code); p = p->next)
		last = p;

	new = (header_p)malloc(sizeof(header_t));
	if (!new) out_of_memory(__FILE__);
	new->text	 = strdup(inhalt);
	if (!new->text) out_of_memory(__FILE__);
	new->header	 = code <= TOTAL_KEYWORDS ? (char *)headers[code] : strdup(header_name);
	new->code	 = code;
	new->other	 = NULL;
	new->next	 = NULL;

	if (p && (p->code == code)) {
		/* Identischer Header, per other verketten */
		/*
		 *  MH: um die Reihenfolge beim Einlesen zu erhalten
		 *      wird jetzt am Ende der other-> angehngt. 
		 */
		for ( ; p->other; p = p->other)
			;
		p->other = new;
		new->next = p->next;
	} else if (!last) {
		/* Am Anfang einfgen */
		new->next = ptr;
		ptr = new;
	} else if (!p) {
		/* Am Ende anhngen */
		last->next = new;
		p=last->other;   /* WO: Die Next-Ptr der identischen Header */
		while (p) {      /* mssen auch verkettet werden */
		   p->next=new;
		   p=p->other;
		}
	} else {
		/* Neuer Header, zwischen last und p einfgen */
		last->next = new;
		new->next = p;
		p=last->other;   /* WO: Die Next-Ptr der identischen Header */
		while (p) {      /* mssen auch verkettet werden */
		   p->next=new;
		   p=p->other;
		}
	}

	return ptr;
}

header_p add_header(char *inhalt, int code, header_p ptr)
{
	if (code > TOTAL_KEYWORDS+1)
		return NULL;

        return internal_add_header(inhalt, code, ptr, headers[code]);
}


