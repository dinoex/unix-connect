prefix   = @prefix@
exec_prefix = @exec_prefix@
DESTDIR  = @BINDIR@
BINDIR   = @BINDIR@
ETCDIR   = @ETCDIR@
SPOOLDIR = @SPOOLDIR@
SHELL    = @shell@
CC       = @CC@
DEFS     = @DEFS@
CPPFLAGS = @CPPFLAGS@ @DEFS@
CFLAGS   = @CFLAGS@
LDFLAGS  = @LDFLAGS@
RANLIB   = @RANLIB@
FQDN     = @FQDN@
USER     = @USER@
GROUP    = @GROUP@
PATHADD  = @PATHADD@
MAIL     = @MAIL@
